{
  "name": "tray-link",
  "version": "1.0.0",
  "description": "An app to manage your projects from the system tray",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prewatch": "yarn prebuild",
    "watch": "tsc -w",
    "lint": "eslint --ext .ts ./src",
    "prestart": "yarn build",
    "start": "electron ./dist/main.js",
    "dev": "nodemon --watch ./ --exec \"yarn start\" -e ts,js,json --ignore dist",
    "format": "prettier --write \"./src/**/*.{js,ts}\""
  },
  "repository": "https://github.com/thejoaov/tray-link",
  "keywords": [
    "Electron",
    "typescript",
    "Visual Studio Code",
    "Terminal",
    "system-tray",
    "project-management",
    "VSCode",
    "terminal",
    "folder-navigation",
    "productivity",
    "code-editing",
    "developer-tools",
    "task-automation",
    "quick-access",
    "file-organization",
    "project-folder",
    "code-organization",
    "easy-navigation",
    "workflow-enhancement",
    "developer-productivity",
    "cross-platform",
    "nodejs"
  ],
  "author": "thejoaov",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "electron": "^24.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "command-exists": "^1.2.9",
    "electron-store": "^8.1.0",
    "execa": "^2.0.0-alpha.0",
    "fix-path": "^4.0.0",
    "rimraf": "^5.0.1",
    "update-electron-app": "^2.0.1",
    "uuid": "^9.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
